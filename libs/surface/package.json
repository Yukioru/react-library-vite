{
  "name": "@uikit/surface",
  "description": "React Library Template / Surface",
  "version": "0.0.1",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "type": "module",
  "keywords": [
    "react",
    "library",
    "surface"
  ],
  "author": "Andrey Kondalov <andrey@kondalov.com>",
  "homepage": "https://github.com/Yukioru/react-library-vite#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/Yukioru/react-library-vite.git"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json",
    "./*": "./dist/*",
    "./**/*": "./dist/**/*"
  },
  "scripts": {
    "build": "tsc && vite build --",
    "prepublishOnly": "npm run build",
    "test": "vitest --passWithNoTests --run --",
    "test:watch": "vitest --",
    "coverage": "vitest run --coverage"
  },
  "peerDependencies": {
    "clsx": "^2",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@uikit/configs": "workspace:^",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "clsx": "^2.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
