{
  "name": "@uikit/button",
  "description": "React Library Template / Button",
  "version": "0.0.1",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "keywords": [
    "react",
    "library",
    "template",
    "vite",
    "typescript",
    "storybook",
    "esm"
  ],
  "author": "Andrey Kondalov <andrey@kondalov.com>",
  "homepage": "https://github.com/Yukioru/react-library-vite#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/Yukioru/react-library-vite.git"
  },
  "files": [
    "dist"
  ],
  "exports": {
