{
  "name": "@uikit/root",
  "description": "React Library Template",
  "version": "0.0.1",
  "type": "module",
  "license": "MIT",
  "private": true,
  "publishConfig": {
    "access": "restricted"
  },
  "keywords": [
    "react",
    "library",
    "template",
    "vite",
    "typescript",
    "storybook",
    "esm"
  ],
  "author": "Andrey Kondalov <andrey@kondalov.com>",
  "homepage": "https://github.com/Yukioru/react-library-vite#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/Yukioru/react-library-vite.git"
  },
  "scripts": {
    "prepare": "husky install",
    "start": "ycmd start",
    "dev": "ycmd dev",
    "build": "ycmd build",
    "test": "ycmd test",
    "prepack": "ycmd prepack",
    "release": "ycmd release"
  },
  "devDependencies": {
    "@uikit/configs": "workspace:^",
    "ycmd": "^0.16.1",
    "typescript": "^5.3.3",
    "storybook": "^7.6.7",
    "@storybook/addon-essentials": "^7.6.7",
    "@storybook/addon-interactions": "^7.6.7",
    "@storybook/addon-links": "^7.6.7",
    "@storybook/addon-onboarding": "^1.0.10",
    "@storybook/blocks": "^7.6.7",
    "@storybook/builder-vite": "^7.6.7",
    "@storybook/react": "^7.6.7",
    "@storybook/react-vite": "^7.6.7",
    "@storybook/test": "^7.6.7",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^14.1.2",
    "@types/node": "^20.10.7",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "@vitest/coverage-istanbul": "^1.1.3",
    "jsdom": "^23.2.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "typescript-plugin-css-modules": "^5.0.2",
    "vite": "^5.0.11",
    "vite-plugin-dts": "^3.7.0",
    "vitest": "^1.1.3"
  },
  "prettier": "@uikit/configs/prettier",
  "eslintConfig": {
    "extends": "@uikit/configs/eslint"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --quiet --fix",
      "prettier --write --ignore-unknown"
    ],
    "*.{json,html}": [
      "prettier --write --ignore-unknown"
    ]
  },
  "ycmd": {
    "scripts": [
      "./scripts",
      "./node_modules/ycmd/scripts"
    ]
  }
}
